{
  "name": "react-jest-enzyme-example",
  "version": "1.0.0",
  "description": "Testing react components with jest and enzyme",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch"
  },
  "author": "IP",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "6.11.3"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "7.4.0",
    "jest": "21.2.1",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "rootDir": "src/test",
    "testRegex": "/src/test/.*test\\.js$",
    "setupFiles": ["<rootDir>/polyfills.js", "<rootDir>/setup.js"]
  }
}
